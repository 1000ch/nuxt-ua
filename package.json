{
  "name": "nuxt-ua",
  "version": "2.0.0",
  "description": "UserAgent detector using platform for Nuxt",
  "type": "module",
  "keywords": [
    "nuxt",
    "platform",
    "ua",
    "useragent",
    "vue"
  ],
  "homepage": "https://github.com/1000ch/nuxt-ua#readme",
  "bugs": {
    "url": "https://github.com/1000ch/nuxt-ua/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1000ch/nuxt-ua.git"
  },
  "license": "MIT",
  "author": "1000ch",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.3",
    "@types/platform": "^1.3.4",
    "platform": "^1.3.6"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.4.0",
    "@nuxt/schema": "3.5.3",
    "@nuxt/test-utils": "3.5.3",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.42.0",
    "nuxt": "3.6.2",
    "vitest": "0.31.0"
  }
}
